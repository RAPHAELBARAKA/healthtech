{
    "version": 2,
    "builds": [
        {
            "src": "/server-side/index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/server-side/app.js",
            "methods":["GET","POST", "PUT", "DELETE", "PATCH", "OPTIONS"],
            "headers":{
                "Access-Control-Allow-Origin": "*"
            }
        }
    ]
}



